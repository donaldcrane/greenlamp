{
  "name": "greenlamp",
  "version": "1.0.0",
  "description": "A user sign in with google or Facebook account app",
  "main": "app.js",
  "scripts": {
    "clean": "npm rm -rf build && npm run build-babel",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "start-dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./src/app.js",
    "test": "cross-env NODE_ENV=test && npm run migration && npm run seeder && nyc --require --reporter=html --reporter=text mocha -r @babel/register ./src/tests/index.js --timeout 50000 --recursive --exit || true ",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov --reporter=lcov | node ./node_modules/coveralls/bin/coveralls.js --verbose",
    "lint": "eslint src/  --fix",
    "migration": "npm run migrate:undo && npm run migrate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all",
    "seeder": "npm run seed:undo && npm run seed",
    "seed": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donaldcrane/greenlamp.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/donaldcrane/greenlamp/issues"
  },
  "homepage": "https://github.com/donaldcrane/greenlamp#readme",
  "dependencies": {
    "@babel/cli": "^7.12.8",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@sendgrid/mail": "^7.4.0",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "coveralls": "^3.1.0",
    "cross": "^1.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "env": "0.0.2",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.6",
    "nyc": "^15.1.0",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^6.2.0",
    "sequelize-test-helpers": "^1.3.2",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0"
  }
}
